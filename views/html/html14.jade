include ../head.jade
	body
		include navbar.jade
		div.container
			div.row
				div.col-sm-3
					include left.jade
				div.col-sm-9
					div.panel.panel-primary
						div.panel-heading
							i.glyphicon.glyphicon-book
							| ページタイトル
						div.panel-body
							div.alert.alert-info ページタイトル
							table.browser-support
								tr
									td
										img(src='images/ic-apple.png')
									td
										img(src='images/ic-android.png')
									td
										img(src='images/ic-firefox.png')
									td
										img(src='images/ic-safari.png')
									td
										img(src='images/ic-chrome.png')
									td
										img(src='images/ic-opera.png')
									td
										img(src='images/ic-ie.png')
							pre
								| &lt;title&gt;...&lt;/title&gt;
							table.table
								tr
									td カテゴリー
									td メタデータコンテンツ
								tr
									td 記述可能な場所
									td
										| &lt;head&gt;の子要素として1つのみ記述可能
										br
										| ※省略不可
										br
										| （&lt;iframe&gt;のsrcdoc属性に記述できる場合や上位プロトコルからタイトルが得られる場合は省略可）
								tr
									td コンテンツモデル
									td テキスト
								tr
									td 構成するボックス
									td none
								tr
									td 属性
									td グローバル 
								tr
									td 標準的なスタイル
									td
										pre
											| html {
											| 	display: none;
											| }
		div#footer=title
		script(src='http://code.jquery.com/jquery.js')
		script(src='js/google-code-prettify/prettify.js')
		script.
			prettyPrint();
		script(src='js/bootstrap.min.js')
		script(src='coffee/list-counter.js')