prefix(prop, val)
    -webkit-{prop} val
    {prop} val

article
    position relative
    width 400px

.turn
    position absolute
    right 0
    bottom 0
    prefix('transform', rotate(0deg))  
    prefix('animation-name', myanim)    
    prefix('animation-duration', 1s)
    prefix('animation-timing-function', linear)
    prefix('animation-iteration-count', infinite)

@keyframes myanim
    0%
        prefix('transform', rotate(0deg))
    100%
        prefix('transform', rotate(360deg))