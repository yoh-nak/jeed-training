include ../head2.jade
	body
		include navbar.jade
		.container
			.row
				.col-sm-3
					include left.jade
				.col-sm-9
					.panel.panel-primary
						.panel-heading
							i.glyphicon.glyphicon-book
							&nbsp;Jade
						.panel-body
							h4 公式サイト
							p
								a(href='http://jade-lang.com/',target='_blank') http://jade-lang.com/
							hr
							h4 公式ドキュメント
							p
								a(href='http://jade-lang.com/reference/',target='_blank') http://jade-lang.com/reference/
							hr
							h4 インストール
							p Windows
							pre
								| npm install -g jade
							p Linux / Mac
							pre
								| sudo npm install -g jade
							hr
							h4 コンパイル
							pre
								| jade &lt;ファイル名&gt;.jade パス pretty
							hr
							h4 Jade/HTML相互変換
							p
								a(href='http://html2jade.herokuapp.com/',target='_blank') http://html2jade.herokuapp.com/
		.footer=title
		script(src='http://code.jquery.com/jquery.js')
		script(src='../js/google-code-prettify/prettify.js')
		script.
			prettyPrint();
		script(src='../js/bootstrap.min.js')
		script(src='../coffee/list-counter.js')