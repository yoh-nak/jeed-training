include ../head2.jade
	body
		include navbar.jade
		.container
			.row
				.col-sm-3
					include left.jade
				.col-sm-9
					.panel.panel-primary
						.panel-heading
							i.glyphicon.glyphicon-book
							&nbsp;Jasmine
						.panel-body
							h4 公式サイト
							p
								a(href='http://pivotal.github.io/jasmine/',target='_blank') http://learnboost.github.io/stylus/
