include ../head.jade
	body
		include navbar.jade
		div.container
			div.row
				div.col-sm-3
					include left.jade
				div.col-sm-9
					div.panel.panel-primary
						div.panel-heading
							i.glyphicon.glyphicon-book
							| 追加・削除
						div.panel-body
							table.browser-support
								tr
									td
										img(src='images/ic-apple.png')
									td
										img(src='images/ic-android.png')
									td
										img(src='images/ic-firefox.png')
									td
										img(src='images/ic-safari.png')
									td
										img(src='images/ic-chrome.png')
									td
										img(src='images/ic-opera.png')
									td
										img(src='images/ic-ie.png')
							.alert.alert-info 追加したコンテンツ
							pre
								| &lt;ins&gt;…&lt;/ins&gt;
							table.table
								tr
									td カテゴリー
									td フローコンテンツ／フレージングコンテンツ／パルパブルコンテンツ
								tr
									td 記述可能な場所
									td
										| フレージングコンテンツが記述できる場所
								tr
									td コンテンツモデル
									td トランスペアレント
								tr
									td 構成するボックス
									td inline
								tr
									td 属性
									td cite／datetime／グローバル
								tr
									td 標準的なスタイル
									td
										pre
											| ins {
											| 	text-decoration: underline;
											| }
							.alert.alert-info 削除したコンテンツ
							pre
								| &lt;del&gt;…&lt;/del&gt;
							table.table
								tr
									td カテゴリー
									td フローコンテンツ／フレージングコンテンツ／パルパブルコンテンツ
								tr
									td 記述可能な場所
									td
										| フレージングコンテンツが記述できる場所
								tr
									td コンテンツモデル
									td トランスペアレント
								tr
									td 構成するボックス
									td inline
								tr
									td 属性
									td cite／datetime／グローバル
								tr
									td 標準的なスタイル
									td
										pre
											| ins {
											| 	text-decoration: line-through;
											| }
		div#footer=title
		script(src='http://code.jquery.com/jquery.js')
		script(src='js/google-code-prettify/prettify.js')
		script.
			prettyPrint();
		script(src='js/bootstrap.min.js')
		script(src='coffee/list-counter.js')