          <div class="panel panel-primary">
          <div class="panel-heading">矩形を表示する</div>
          <div class="panel-body">

<p>
<a href="demo/webapi/googlemap-24/index.html" target="_blank" class="btn btn-default">
<i class="glyphicon glyphicon-play"></i> デモ
</a>
</p>

<pre class="prettyprint linenums">
&lt;!DOCTYPE html&gt;
&lt;html lang="ja"&gt;
  &lt;head&gt;
    &lt;meta charset="utf-8"&gt;
    &lt;title&gt;&lt;/title&gt;
    &lt;script src="http://code.jquery.com/jquery-1.11.0.min.js"&gt;&lt;/script&gt;
    &lt;script src="http://maps.google.com/maps/api/js?sensor=true&language=ja"&gt;&lt;/script&gt;
    &lt;script&gt;
      $(function(){
       var latlng = new google.maps.LatLng(34.705776,135.499138);
       var options = {
        zoom: 14,
        center: latlng,
        mapTypeId: google.maps.MapTypeId.ROADMAP
       };
       var map = new google.maps.Map(document.getElementById('map'), options);
       
       var boundsRect = new google.maps.LatLngBounds(
        new google.maps.LatLng(34.687146,135.482315), //矩形左下の緯度経度
        new google.maps.LatLng(34.721015,135.521652) //矩形右上の緯度経度
       );
       
       var rectangle = new google.maps.Rectangle({
        bounds: boundsRect,
        map: map, //マップオブジェクト
        center: latlng, //中心の緯度経度
        strokeWeight: 1, //線の太さ
        strokeColor: '#ff0000', //線の色
        fillColor: 'ffff00', //塗りの色
        strokeOpacity: 0.5, //線の不透明度
        fillOpacity: 0.3, //塗りの不透明度
        clickable: false //クリックできるかどうか
       });
      });
    &lt;/script&gt;
    &lt;style&gt;
      html,body{
       width:100%;
       height:100%;
       margin:0;
      }
      #map{
       width:100%;
       height:100%;
      }
    &lt;/style&gt;
  &lt;/head&gt;
  &lt;body&gt;
    &lt;div id="map"&gt;&lt;/div&gt;
  &lt;/body&gt;
&lt;/html&gt;
