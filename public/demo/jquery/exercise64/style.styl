prefix(prop, val)
	-webkit-{prop} val
	{prop} val

.even
	background-color transparent
.odd
	background-color transparent

.load
	prefix('transition-property', all)	
	prefix('transition-duration', 500ms)
	prefix('transition-timing-function', ease-in-out)
	&.even
		background-color red
	&.odd
		background-color blue
