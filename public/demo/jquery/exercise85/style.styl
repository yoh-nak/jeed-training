prefix(prop, val)
	-webkit-{prop} val
	{prop} val

li
	opacity 1.0
.turn
	prefix('transform', rotateX(0deg))	
	prefix('animation-name', myanim)	
	prefix('animation-duration', 1s)
.fade
	opacity 0.2
	prefix('transition-property', all)	
	prefix('transition-duration', 500ms)
	prefix('transition-timing-function', ease-in-out)

@keyframes myanim
	0%
		prefix('transform', rotateX(0deg))
	50%
		prefix('transform', rotateX(180deg))
	100%
		prefix('transform', rotateX(360deg))
