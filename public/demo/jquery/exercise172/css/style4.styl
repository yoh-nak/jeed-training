#floatWindow
    display none
    width 500px
    height 400px
    a.close img
        border none
    dl
        width 100%
        height 100%
        background url("../img/floatWindow.png")
        margin 0
        dt
            height 25px
            line-height 25px
            text-indent 1em
            color #FFFFFF
            font-weight bold
        dd
            margin 0
            padding 2em
            line-height 1.5
            text-indent 1em
