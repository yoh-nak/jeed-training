&lt;!DOCTYPE html&gt;
&lt;html lang="ja"&gt;
  &lt;head&gt;
    &lt;meta charset="utf-8"&gt;
    &lt;title&gt;&lt;/title&gt;
    &lt;script src="http://code.jquery.com/jquery-1.10.2.min.js"&gt;&lt;/script&gt;
    &lt;script src="http://maps.google.com/maps/api/js?sensor=true&language=ja"&gt;&lt;/script&gt;
    &lt;script&gt;
      $(function(){
       var latlng = new google.maps.LatLng(34.686334,135.526822);
       var options = {
        zoom: 15,
        center: latlng,
        mapTypeId: google.maps.MapTypeId.ROADMAP
       };
       var map = new google.maps.Map(document.getElementById('map'), options);
       
       var polygon = new google.maps.Polygon({
        paths: [
         new google.maps.LatLng(34.691486,135.525406),
         new google.maps.LatLng(34.690639,135.527852),
         new google.maps.LatLng(34.690639,135.529354),
         new google.maps.LatLng(34.689686,135.532401),
         new google.maps.LatLng(34.689545,135.534375),
         new google.maps.LatLng(34.681711,135.533517),
         new google.maps.LatLng(34.682382,135.52356),
         new google.maps.LatLng(34.683229,135.521672),
         new google.maps.LatLng(34.688063,135.52017),
         new google.maps.LatLng(34.689228,135.521801),
         new google.maps.LatLng(34.689369,135.522745),
         new google.maps.LatLng(34.69071,135.522702)
        ],
        map: map, //マップオブジェクト
        center: latlng, //中心の緯度経度
        strokeWeight: 5, //線の太さ
        strokeColor: '#ff0000', //線の色
        fillColor: 'ffff00', //塗りの色
        strokeOpacity: 0.5, //線の不透明度
        fillOpacity: 0.3, //塗りの不透明度
        clickable: false //クリックできるかどうか
       });
      });
    &lt;/script&gt;
    &lt;style&gt;
      html,body{
       width:100%;
       height:100%;
       margin:0;
      }
      #map{
       width:100%;
       height:100%;
      }
    &lt;/style&gt;
  &lt;/head&gt;
  &lt;body&gt;
    &lt;div id="map"&gt;&lt;/div&gt;
  &lt;/body&gt;
&lt;/html&gt;
