prefix(prop, val)
	-webkit-{prop} val
	{prop} val

div
	position relative
	width 100%
	background-color aqua
	padding 16px
	span
		position absolute
		display inline-block
		bottom 0%
		left 316px
		background-color orange
		width 100px
		height 20px
		border-radius 0%
		text-align center
		line-height 20px
article
	width 400px
.after
	position absolute
	display inline-block
	bottom 70%
	left 80%
	background-color yellow
	width 100px
	height 100px
	border-radius 50%
	text-align center
	line-height 100px
	prefix('transition-property', all)	
	prefix('transition-duration', 500ms)
	prefix('transition-timing-function', ease-in-out)