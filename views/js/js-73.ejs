        <div class="col-sm-9">
          <div class="panel panel-primary">
          <div class="panel-heading">
            <i class="glyphicon glyphicon-book"></i> JavaScript資料</div>
          <div class="panel-body">

<pre>

繰り返し



for(初期化の式;繰り返し条件の式;加算の式){
     繰り返す処理
}


var a = 0;
for(var i = 1; i <= 10; i++){
     a = a + i;
}
document.writeln(a);


while(繰り返し条件の式){
     繰り返す処理
}

var a = 3;
while(a < 10){
     a = a + 3;
}


do{
     繰り返す処理
}while(繰り返し条件の式);


var a = 10;
do{
     a = a + 3;
}while(a < 10);
document.writeln(a);



var a = 0;
for(var i = 0; i , 10; i++){
     if(a >= 10) break;
     if(i % 2 == 0) continue;
     a = a + i;
}
document.writeln(a);


</pre>
