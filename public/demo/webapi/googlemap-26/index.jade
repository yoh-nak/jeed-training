doctype html
html(lang='ja')
    head
        meta(charset='utf-8')
        title
        script(src='http://code.jquery.com/jquery-1.11.0.min.js')
        script(src!='http://maps.google.com/maps/api/js?sensor=true&language=ja')
        style
            :stylus
                html,body
                    width 100%
                    height 100%
                    margin 0
                #map
                    width 100%
                    height 100%
        script
            :js
                (function(){
                    $(function(){
                        var latlng = new google.maps.LatLng(34.666358,135.5);
                        var options = {
                            zoom: 11,
                            center: latlng,
                            mapTypeId: google.maps.MapTypeId.ROADMAP
                        };
                        var map = new google.maps.Map($('#map').get(0), options);

                        var polyline = new google.maps.Polyline({
                            path: [
                                new google.maps.LatLng(34.757727,135.496824),
                                new google.maps.LatLng(34.740978,135.498455),
                                new google.maps.LatLng(34.733113,135.498627),
                                new google.maps.LatLng(34.726447,135.498584),
                                new google.maps.LatLng(34.711068,135.496953),
                                new google.maps.LatLng(34.702918,135.497768),
                                new google.maps.LatLng(34.692509,135.500644),
                                new google.maps.LatLng(34.681993,135.500772),
                                new google.maps.LatLng(34.675147,135.500215),
                                new google.maps.LatLng(34.666394,135.500429),
                                new google.maps.LatLng(34.656086,135.497554),
                                new google.maps.LatLng(34.649308,135.503862),
                                new google.maps.LatLng(34.647296,135.513561),
                                new google.maps.LatLng(34.633667,135.517037),
                                new google.maps.LatLng(34.621554,135.515192),
                                new google.maps.LatLng(34.611171,135.512445),
                                new google.maps.LatLng(34.598949,135.513003),
                                new google.maps.LatLng(34.581744,135.516479),
                                new google.maps.LatLng(34.567715,135.51502),
                                new google.maps.LatLng(34.555982,135.505407)
                            ],
                            map: map, //マップオブジェクト
                            center: latlng, //中心の緯度経度
                            strokeWeight: 8, //線の太さ
                            strokeColor: '#ff0000', //線の色
                            strokeOpacity: 0.8, //線の不透明度
                            zIndex: 1 //重なり順
                        });
                    });
                })()
    body
        #map