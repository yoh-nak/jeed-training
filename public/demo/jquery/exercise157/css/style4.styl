w = 650px
h = 488px

body
    margin 0
h1
    margin 20px 50px
    font-size large
    border-left 10px solid #7BAEB5
    border-bottom 1px solid #7BAEB5
    padding 10px
    width 600px
ul
    width 700px
    li
        float left
        list-style-type none
        img
            border 0
            margin 10px
#grayLayer
    display none
    position fixed
    left 0
    top 0
    height 100%
    width 100%
    background-color black
    opacity 0.60
#overLayer
    display none
    position  fixed
    top 50%
    left 50%
    margin-top -(h/2)
    margin-left -(w/2)
    img
    	width w
    	height h