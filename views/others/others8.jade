extends ../layout

block link
    link(href='../css/bootstrap.min.css',rel='stylesheet',media='screen')
    link(href='../css/bootstrap-theme.min.css',rel='stylesheet',media='screen')
    link(href='../js/dtree/dtree.css', rel='stylesheet', media='screen')
    link(href='../css/style.css',rel='stylesheet',media='screen')
    link(href='../js/google-code-prettify/prettify.css',rel='stylesheet',media='screen')

block content
    include navbar.jade
    .container
        .row
            .col-sm-3
                include left.jade
            .col-sm-9
                .panel.panel-primary
                    .panel-heading
                        i.glyphicon.glyphicon-book
                        &nbsp;Chrome Developer Toolsの使い方
                    .panel-body
                        h4 公式サイト
                        p
                            a(href='https://developers.google.com/chrome-developer-tools/?hl=ja',target='_blank') https://developers.google.com/chrome-developer-tools/?hl=ja
                        hr
                        h4 ドットインストール
                        p
                            a(href='http://dotinstall.com/lessons/basic_chrome_dev', target='_blank') Chrome Developer Tools入門
                        hr
                        .alert.alert-info Chromeアプリ
                        h4 Pendule
                        p
                            | CSSを解除する
                            br
                            a(href='https://chrome.google.com/webstore/detail/pendule/gbkffbkamcejhkcaocmkdeiiccpmjfdi',target='_blank') https://chrome.google.com/webstore/detail/pendule/gbkffbkamcejhkcaocmkdeiiccpmjfdi
                        h4 HTML5 Outliner
                        p
                            | HTML5のアウトラインを表示する
                            br
                            a(href='https://chrome.google.com/webstore/detail/html5-outliner/afoibpobokebhgfnknfndkgemglggomo',target='_blank') https://chrome.google.com/webstore/detail/html5-outliner/afoibpobokebhgfnknfndkgemglggomo