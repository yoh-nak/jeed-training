!!!
html(lang='ja')
	head
		meta(name='viewport',content='width=device-width,initial-scale=1.0')
		link(href='css/bootstrap.min.css',rel='stylesheet',media='screen')
		link(href='js/dtree/dtree.css', rel='stylesheet', media='screen')
		link(href='less/common.less',rel='stylesheet',media='screen')
		link(href='js/google-code-prettify/prettify.css',rel='stylesheet',media='screen')
		title=title
		:coffeescript
		:less
	body
		nav.navbar.navbar-default
			div.navbar-header
				button.navbar-toggle(data-toggle='collapse',data-target='.target')
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand(href='/')=title
			div.collapse.navbar-collapse.target
				ul.nav.navbar-nav
					li.active
						a(href='css3-1') CSS3
					li
						a(href='#') Sass
				ul.nav.navbar-nav.navbar-right
					li
						a(href="http://sass-lang.com/",target="_blank") Sass
					li
						a(href="http://compass-style.org/",target="_blank") Compass
					li
						a(href="http://lesscss.org/",target="_blank") Less
					li
						a(href="http://learnboost.github.io/stylus/",target="_blank") Stylus
		div.container
			div.row
				div.col-sm-3
				div.col-sm-6
		div#footer=title
		script(src='http://code.jquery.com/jquery.js')
		script(src='js/bootstrap.min.js')