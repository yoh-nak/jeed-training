include ../head2.jade
	body
		include navbar.jade
		.container
			.row
				.col-sm-3
					include left.jade
				.col-sm-9
					.panel.panel-primary
						.panel-heading
							i.glyphicon.glyphicon-book
							&nbsp;Bower
						.panel-body
							h4 公式サイト
							p
								a(href='http://bower.io/',target='_blank') http://bower.io/
							hr
							h4 インストール
							p Windows
							pre
								| npm install -g bower
							p Linux / Mac
							pre
								| sudo npm install -g bower
							hr
							h4 アンインストール
							p Windows
							pre
								| npm uninstall -g bower
							p Linux / Mac
							pre
								| sudo npm uninstall -g bower
							hr
							h4 アップデート
							pre bower update
							h4 jQuery
							pre bower install jquery
							pre bower install jquery#1.9.2
							pre bower help
							pre bower ls
							pre bower info jquery
							pre bower search jquery
							pre bower lookup
							pre bower register
							pre bower uninstall jquery
							h4 jQuery Mobile
							pre bower install jquery-mobile
							h4 underscore.js
							pre bower install underscore
							h4 backbone.js
							pre bower install backbone
		.footer=title
		script(src='http://code.jquery.com/jquery-1.10.2.min.js')
		script(src='../js/google-code-prettify/prettify.js')
		script.
			prettyPrint();
		script(src='../js/bootstrap.min.js')
		script(src='../coffee/list-counter.js')