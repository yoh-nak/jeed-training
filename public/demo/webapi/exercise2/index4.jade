        script
            :js
                (function(){
                    $(function(){
                        $('input').click(function(){
                            $.ajax({
                                type:'GET',
                                url:'sample.html',
                                success:function(data){
                                    $('p').html(data);
                                }
                            });
                        });
                    });
                })()
