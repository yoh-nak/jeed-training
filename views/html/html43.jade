include ../head2.jade
	body
		include navbar.jade
		.container
			.row
				.col-sm-3
					include left.jade
				.col-sm-9
					.panel.panel-primary
						.panel-heading
							i.glyphicon.glyphicon-book
							| &nbsp;コマンドリスト
						.panel-body
							.alert.alert-info コマンドリスト
							table.browser-support
								tr
									td
										img(src='../images/ic-apple.png')
										| （※）
									td
										img(src='../images/ic-android.png')
										| （※）
									td
										img(src='../images/ic-firefox.png')
										| （※）
									td
										img(src='../images/ic-safari.png')
										| （※）
									td
										img(src='../images/ic-chrome.png')
										| （※）
									td
										img(src='../images/ic-opera.png')
										| （※）
									td
										img(src='../images/ic-ie.png')
										| （※）
							p ※ 一部非対応
							pre
								| &lt;menu&gt;…&lt;/menu&gt;
							table.table
								tr
									td カテゴリー
									td フローコンテンツ／インタラクティブコンテンツ（type属性の値がtoolbarの場合）／パルパブルコンテンツ（type属性の値がtoolbarまたはtype属性を省略した場合）
								tr
									td 記述可能な場所
									td
										| フローコンテンツが記述できる場所
								tr
									td コンテンツモデル
									td 0個以上の&lt;li&gt;、またはフローコンテンツ
								tr
									td 構成するボックス
									td -（ブロックボックスを構成することが期待される）
								tr
									td 属性
									td type／label／グローバル
								tr
									td 標準的なスタイル
									td
										pre
											| menu {
											| 	display: block;
											| 	margin-top: 1em;
											| 	margin-bottom: 1em;
											| 	padding-left: 40em;
											| 	list-style-type: disc
											| }
											| menu[type=context]{
											| 	display: none;
											| }
							pre
								| &lt;command&gt;
							table.table
								tr
									td カテゴリー
									td メタデータコンテンツ／フローコンテンツ／フレージングコンテンツ
								tr
									td 記述可能な場所
									td
										| メタデータコンテンツまたはフレージングコンテンツが記述できる場所
								tr
									td コンテンツモデル
									td 空
								tr
									td 構成するボックス
									td none
								tr
									td 属性
									td type／label／icon／disabled／checked／radiogroup／command／グローバル
								tr
									td 標準的なスタイル
									td
										pre
											| command {
											| 	display: none;
											| }
		.footer=title
		script(src='http://code.jquery.com/jquery.js')
		script(src='../js/google-code-prettify/prettify.js')
		script.
			prettyPrint();
		script(src='../js/bootstrap.min.js')
		script(src='../coffee/list-counter.js')