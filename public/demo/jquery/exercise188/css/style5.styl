ul
    list-style-type none
    margin-top 0
    margin-bottom 0
    padding-left 0

#container
    width 650px
    margin 100px auto

#slideGalley
    width 650px
    position relative
    overflow hidden

#slide
    width 10000px
    overflow hidden
    li
        float left

#paging
    text-align center
    position absolute
    width 100%
    bottom 10px
    li
        display inline-block
        width 20px
        height 20px
        background url(../img/page.png)
        &.active
            background url(../img/page_active.png)

#nav
    .prev
        position absolute
        left 10px
        top 50%
        margin-top -50px

    .next
        position  absolute
        right 10px
        top 50%
        margin-top -50px

