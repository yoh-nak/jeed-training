prefix(prop, val)
    -webkit-{prop} val
    {prop} val

size()
    100px

img
    width size()
    height @width
    opacity 0.5
    prefix('transition-property', all)  
    prefix('transition-duration', 500ms)
    prefix('transition-timing-function', ease-in-out)
    &.large
        width size()*2
        height @width
        opacity 1.0
