#container
	width 500px
	margin 50px auto

ul.tab
	padding-left 0

	li
		list-style-type none
		width 100px
		height 40px
		float left
		a
			outline none
			background url("../img/tab.jpg")
			display block
			color blue
			line-height 40px
			text-align center
			&.selected
				background url("../img/tab_selected.jpg")
				text-decoration none
				color #333
				cursor default
