                        h4 オブジェクトのネスト（二次元オブジェクト）
                        pre(class='prettyprint linenums:1')
                            :code
                                var grade = {
                                    '数学': {
                                        '数学Ⅰ': 'B',
                                        '数学Ⅱ': 'A',
                                        '数学Ⅲ': 'C'
                                    },
                                    '物理': {
                                        '物理ⅠB': 'A',
                                        '物理Ⅱ': 'B'
                                    }
                                }
                        pre(class='prettyprint linenums:1')
                            :code
                                grade =
                                  "数学":
                                    "数学Ⅰ": "B"
                                    "数学Ⅱ": "A"
                                    "数学Ⅲ": "C"

                                  "物理":
                                    "物理ⅠB": "A"
                                    "物理Ⅱ": "B"
                        p 外側のオブジェクトと内側のオブジェクトをすべて取得する
                        pre(class='prettyprint linenums:1')
                            :code
                                var grade = {
                                    '数学': {
                                        '数学Ⅰ': 'B',
                                        '数学Ⅱ': 'A',
                                        '数学Ⅲ': 'C'
                                    },
                                    '物理': {
                                        '物理ⅠB': 'A',
                                        '物理Ⅱ': 'B'
                                    }
                                }

                                for(var a in grade){
                                    console.log(a); //外側のキーを取得
                                    console.log(grade[a]); //外側の値を取得
                                    for(var b in grade[a]){
                                        console.log(b); //内側のキーを取得
                                        console.log(grade[a][b]); //内側の値を取得
                                    }
                                }
                        pre(class='prettyprint linenums:1')
                            :code
                                grade =
                                  "数学":
                                    "数学Ⅰ": "B"
                                    "数学Ⅱ": "A"
                                    "数学Ⅲ": "C"

                                  "物理":
                                    "物理ⅠB": "A"
                                    "物理Ⅱ": "B"

                                for a of grade
                                  console.log a #外側のキーを取得
                                  console.log grade[a] #外側の値を取得
                                  for b of grade[a]
                                    console.log b #内側のキーを取得
                                    console.log grade[a][b] #内側の値を取得
